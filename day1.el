;; Part 2

(defun day1-part-2 (input)
  "Produce the first place that he enters the basement given INPUT."
  (interactive "sInput: ")
  (let ((opens 0)
        (closes 0)
        (i 0)
        pos)
    (while (and (null pos)
                (< i (length input)))
      (when (eq (aref input i) ?\()
        (incf opens))
      (when (eq (aref input i) ?\))
        (incf closes))
      (when (< (- opens closes) 0)
        (setq pos (1+ i)))
      (incf i))
    pos))

